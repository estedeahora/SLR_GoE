@article{Wacquant2017,
  title = {Bourdieu Viene a La Ciudad: {{Pertinencia}}, Principios, Aplicaciones},
  author = {Wacquant, Lo{\"i}c},
  year = {2017},
  journal = {Eure},
  volume = {43},
  number = {129},
