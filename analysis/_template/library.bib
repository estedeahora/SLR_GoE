@book{DiVirgilio2014c,
  title = {Ciudades Latinoamericanas: Desigualdad, Segregaci{\'o}n y Tolerancia},
  editor = {Di Virgilio, Mar{\'i}a Mercedes and Perelman, Mariano},
  year = {2014},
  publisher = {CLACSO},
  address = {Buenos Aires},
  abstract = {Escriben: Soldano, Daniela Ruiz, Juan Gledhill, John Hita, Mar{\'i}a Gabriela Toledo, Enrique de la Garza Hern{\'a}ndez Romo, Marcela Govoreanu, Morgane T{\"o}pfer, Tobias Gonz{\'a}lez G{\'o}mez, Carmen Imelda W{\'o}jtowicz, Miros{\l}aw Sassone, Susana Mar{\'i}a Matossian, Brenda Hiernaux, Daniel EL OBJETIVO DE ESTE LIBRO ES avanzar en la comprensi{\'o}n de los procesos de segregaci{\'o}n y desigualdad social poniendo el foco en los modos en los que ciertos grupos sociales acceden o buscan acce- der a la ciudad. El supuesto sobre el que trabajamos es que los modos de habitar, transitar y circular en la ciudad --en tanto {\'a}mbito de repro- ducci{\'o}n social- contribuyen a (re)producir la desigualdad socio-ur- bana. Entendemos que la desigualdad y los procesos de segregaci{\'o}n se construyen con base en elementos materiales y simb{\'o}licos, hist{\'o}- ricamente producidos, social y territorialmente contextualizados. De este modo, conceptualizamos a la desigualdad como un fen{\'o}meno socio-territorial. Es decir, como un fen{\'o}meno socialmente producido que tiene manifestaciones y articulaciones espaciales claras y que, a su vez, se nutre de ellas. En este enfoque, entonces, la desigualdad social tiene su correlato territorial. Asimismo, las manifestaciones y articulaciones espaciales de la desigualdad se ven afectadas por la doble naturaleza del territorio: territorios fijos y territorios m{\'o}viles (Sack, 1986).},
  isbn = {978-987-722-019-3},
  keywords = {dirichlet problem,higher-order poisson equation,US: Segregacion urbana},
