@article{Joo2020,
  title = {Navigating through the r Packages for Movement},
  author = {Joo, Roc{\'i}o and Boone, Matthew E. and Clay, Thomas A. and Patrick, Samantha C. and Clusella-Trullas, Susana and Basille, Mathieu},
