@article{Stoyan2017,
  title = {Mark Variograms for Spatio-Temporal Point Processes},
  author = {Stoyan, Dietrich and {Rodr{\'i}guez-Cort{\'e}s}, Francisco J. and Mateu, Jorge and Gille, Wilfried},
  year = {2017},
  journal = {Spatial Statistics},
  volume = {20},
  pages = {125--147},
  issn = {22116753},
