@incollection{Pattillo2014,
  title = {High-{{Stakes Choosing}}},
  booktitle = {Choosing {{Homes}}, {{Choosing Schools}}},
  author = {Pattillo, Mary and {Delale-O'Connor}, Lori and Butts, Felicia},
