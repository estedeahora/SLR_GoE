@incollection{Erikson2019,
  title = {How Does Education Depend on Social Origin?},
  booktitle = {Research {{Handbook}} on the {{Sociology}} of {{Education}}},
  author = {Erikson, Robert},
  editor = {Becker, Rolf},
  year = {2019},
  pages = {35--56},
  publisher = {Edward Elgar Publishing},
  address = {Cheltenham, UK},
  doi = {10.4337/9781788110426.00010},
  abstract = {La educaci{\'o}n tiene un papel importante en la vida del mundo moderno y la cuesti{\'o}n de identificar c{\'o}mo el logro educativo est{\'a} determinado por el origen social es fundamental para comprender c{\'o}mo se sit{\'u}an las personas en la estructura social. Reducir el modo en que el logro educativo de las personas depende de su origen social es un objetivo pol{\'i}tico importante. La desigualdad de oportunidades educativas (IEO), considerada como la asociaci{\'o}n entre la educaci{\'o}n, la clase social, el estatus o los ingresos de los padres, y el logro educativo de los hijos en la edad adulta ha sido ampliamente estudiada durante el {\'u}ltimo medio siglo. El origen social de hombres y mujeres parece representar no m{\'a}s del 25 por ciento de la variaci{\'o}n en su nivel educativo en cualquier sociedad avanzada y en la mayor{\'i}a de los pa{\'i}ses representa claramente menos. En la mayor{\'i}a de los pa{\'i}ses, la asociaci{\'o}n entre origen y educaci{\'o}n parece haber disminuido en el per{\'i}odo posterior a la Segunda Guerra Mundial, disminuci{\'o}n que, en muchos pa{\'i}ses, parece haberse estancado con las cohortes nacidas entre 1950 y 1960. Dados los otros factores, la educaci{\'o}n de los padres, la clase social , el estatus social y los ingresos tienen efectos separados en la educaci{\'o}n de los ni{\~n}os. El papel del rendimiento educativo en la transici{\'o}n al siguiente nivel educativo (efectos primarios) y el papel de la elecci{\'o}n dado el rendimiento (efectos secundarios) parecen diferir entre los ni{\~n}os de diferentes or{\'i}genes sociales, en todos los pa{\'i}ses. En la mayor{\'i}a de los pa{\'i}ses, los efectos primarios explican una mayor parte de la variaci{\'o}n en las tasas de transici{\'o}n en el primer obst{\'a}culo, desde la educaci{\'o}n obligatoria hasta la educaci{\'o}n secundaria superior acad{\'e}mica, que en el segundo, desde la educaci{\'o}n secundaria superior acad{\'e}mica hasta la educaci{\'o}n terciaria. La asociaci{\'o}n entre el origen social y el nivel educativo es ligeramente menor entre las mujeres que entre los hombres, mientras que los patrones son bastante similares. La proporci{\'o}n de mujeres que han alcanzado la educaci{\'o}n superior ha aumentado mucho y considerablemente m{\'a}s que la de los hombres. Sin embargo, la expansi{\'o}n parece no haber tenido una influencia sustancial en la IEO entre mujeres u hombres. Los hijos de inmigrantes generalmente obtienen menos educaci{\'o}n que los ni{\~n}os de la poblaci{\'o}n mayoritaria, pero los factores de origen social tienen efectos globales similares en los dos grupos. Los ni{\~n}os inmigrantes tienden a desempe{\~n}arse peor en la escuela, pero dado su desempe{\~n}o, es m{\'a}s probable que opten por continuar con el siguiente nivel educativo. La desigualdad de oportunidades educativas tiende a ser mayor en los sistemas educativos estratificados donde los ni{\~n}os se dividen en v{\'i}as separadas en la escuela a una edad relativamente temprana. En sistemas estandarizados donde el plan de estudios es similar en todas las escuelas y donde los estudiantes tienen que aprobar ex{\'a}menes centrales, la IEO puede ser m{\'a}s d{\'e}bil.},
  keywords = {_sin-pdf}
}

