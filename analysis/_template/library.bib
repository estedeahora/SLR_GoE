@article{VergaraParra2018,
  title = {Segregaci{\'o}n Socioterritorial En La {{Regi{\'o}n Metropolitana}} de {{Buenos Aires}}: An{\'a}lisis Espacial Intraurbano, Caracter{\'i}sticas y Evoluci{\'o}n Reciente, 2001-2010},
