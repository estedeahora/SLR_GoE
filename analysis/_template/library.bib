@article{Josse2016,
  title = {{{missMDA}}: {{A}} Package for Handling Missing Values in Multivariate Data Analysis},
  author = {Josse, Julie and Husson, Fran{\c c}ois},
  year = {2016},
  journal = {Journal of Statistical Software},
  volume = {70},
  number = {1},
  issn = {15487660},
