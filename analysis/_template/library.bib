@phdthesis{OlmedoReinoso2007,
  type = {Tesis Doctoral},
  title = {Las {{Estrategias}} de {{Elecci{\'o}n}} de {{Centro Educativo}} En Las {{Familias}} de {{Clase Media}}. {{Estudio}} de La {{Incidencia Social}} En Un {{Mercado Educativo Local}}},
  author = {Olmedo Reinoso, Antonio},
  year = {2007},
  abstract = {Esta tesis doctoral parte del inter{\'e}s por determinar el papel que una variable tradicional, como es la clase social, sigue ejerciendo en la determinaci{\'o}n de distintas din{\'a}micas sociales y educativas. En concreto, analiza el impacto del proceso de elecci{\'o}n de centro educativo por parte de las familias de clase media como actores sociales y pol{\'i}ticos. Para ello ha sido necesaria la redefinici{\'o}n del propio concepto de clase social y, en particular, de las clases medias en el marco de la actual sociedad de la informaci{\'o}n y el conocimiento.La investigaci{\'o}n realizada ha sido dividida en tres partes en base al planteamiento metodol{\'o}gico predominante en cada una de ellas. En un primer momento, se presentan los resultados de un estudio de corte cuantitativo con el que se pone de manifiesto la pervivencia de la influencia de la clase social a lo largo de las distintas etapas del sistema educativo en el marco de un mercado educativo local. Se observa c{\'o}mo la segregaci{\'o}n escolar, las calificaciones acad{\'e}micas y la existencia de distintas expectativas laborales y educativas responden en alto grado al efecto de la clase social familiar a la que los alumnos y alumnas encuestados pertenecen. En segundo lugar, mediante un an{\'a}lisis de contenido, se analizan las pol{\'i}ticas p{\'u}blicas de la etapa democr{\'a}tica en relaci{\'o}n a la elecci{\'o}n de centro y participaci{\'o}n de las familias en los mismos. De esta forma, se observa la tendencia a la paulatina creaci{\'o}n de un sistema de quasimercado en el que los padres son considerados como consumidores de educaci{\'o}n y gozan de mayor libertad y responsabilidad con respecto al proceso de elecci{\'o}n de centro. Por {\'u}ltimo, desde una perspectiva cualitativa, se exponen los resultados obtenidos del an{\'a}lisis de una serie de entrevistas en profundidad. En esta fase fueron entrevistados un grupo de padres y madres de clase media en relaci{\'o}n a la elecci{\'o}n de centro educativo para sus hijos. Con este an{\'a}lisis se pone de manifiesto c{\'o}mo el proceso de elecci{\'o}n, y las distintas estrategias que de {\'e}ste se derivan, responden a din{\'a}micas m{\'a}s amplias de creaci{\'o}n de identidades de clase, teniendo como objetivo la reproducci{\'o}n de la posici{\'o}n social familiar entre las distintas generaciones.},
  school = {Universidad de Granada},
  keywords = {clase de servicio,clases medias,DE: School choice,eleccion,mercado educativo},
  file = {C:\Users\este.de.ahora\Documents\Biblioteca\Olmedo Reinoso - 2007 - Las Estrategias de Elección de Centro Educativo en las Familias de Clase Media.pdf}
}

@article{OlmedoReinoso2008,
  title = {Las Familias de Clase Media y Elecci{\'o}n de Centro: El Orden Instrumental Como Condici{\'o}n Necesaria Pero No Suficiente},
  author = {Olmedo Reinoso, Antonio and Santa Cruz Grau, Eduardo},
  year = {2008},
  journal = {Profesorado: Revista de curriculum y formaci{\'o}n del profesorado},
  volume = {12},
  number = {2},
  pages = {1--31},
  abstract = {En este art{\'i}culo se presentan los resultados de un proyecto de investigaci{\'o}n cualitativo centrado en el proceso de elecci{\'o}n de centro por parte de las familias de clase media de la ciudad de Granada. En concreto, se analiza c{\'o}mo se produce la valoraci{\'o}n de los distintos centros educativos que hacen las familias, cu{\'a}l es su discurso en torno a los mismos y qu{\'e} tipo de estrategias ponen en pr{\'a}ctica. Para llevar a cabo este an{\'a}lisis resulta de gran utilidad el concepto de ``cultura escolar'' desarrollado por Bernstein y su relaci{\'o}n con los {\'o}rdenes que este mismo autor define, el orden instrumental y el orden expresivo. Aqu{\'i} abordaremos el primero de ellos, aunque se debe tener presente que ambos se presentan {\'i}ntimamente relacionados, constituyendo el centro del proceso de valoraci{\'o}n de los centros. Como se aprecia en nuestro estudio, los padres de clase media son conscientes de la importancia del ascenso educativo y la obtenci{\'o}n de las distintas calificaciones y credenciales que faciliten el acceso a las siguientes etapas. As{\'i}, un ``buen centro'' ser{\'a} aqu{\'e}l que garantiza unos buenos resultados, promocionando e instando a sus alumnos a no aceptar expectativas por debajo de sus posibilidades, optando en todo momento a las posiciones m{\'a}s altas que su ``potencial'', tanto personal como familiar, les permita. Se observan constantes referencias a la influencia de los compa{\~n}eros y el grupo de iguales en el mantenimiento de dichas expectativas y aspiraciones, la preparaci{\'o}n del profesorado o las metodolog{\'i}as utilizadas por los mismos. Por {\'u}ltimo, la educaci{\'o}n privada se presenta como un ``refugio'', gracias a la facilidad de control sobre el ambiente escolar de sus hijos y un cierto nivel de selecci{\'o}n del alumnado en funci{\'o}n a la clase social.},
  keywords = {Clase social,DE: School choice,Eleccion de centro,Instrumental order,Orden instrumental,Reproduccion social,School choice,Social class,Social reproduction},
  file = {C:\Users\este.de.ahora\Documents\Biblioteca\Olmedo Reinoso, Santa Cruz Grau - 2008 - Las familias de clase media y elección de centro.pdf}
}

