@article{Marom2014,
  title = {Relating a {{City}}'s {{History}} and {{Geography}} with {{Bourdieu}}: {{One Hundred Years}} of {{Spatial Distinction}} in {{Tel Aviv}}},
  shorttitle = {Relating a {{City}}'s {{History}} and {{Geography}} with {{Bourdieu}}},
  author = {Marom, Nathan},
