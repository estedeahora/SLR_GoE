@article{Koslinski2012,
  title = {Novos Olhares Para as Desigualdades de Oportunidades Educacionais: A Segrega{\c c}{\~a}o Residencial e a Rela{\c c}{\~a}o Favela-Asfalto No Contexto Carioca},
  shorttitle = {Novos Olhares Para as Desigualdades de Oportunidades Educacionais},
  author = {Koslinski, Mariane Campelo and Alves, Fatima},
  year = {2012},
  month = sep,
  journal = {Educa{\c c}{\~a}o \& Sociedade},
  volume = {33},
  number = {120},
  pages = {805--831},
  issn = {0101-7330},
  doi = {10.1590/S0101-73302012000300009},
  urldate = {2024-11-21},
  abstract = {Desde o per{\'i}odo do p{\'o}s-guerra, a discuss{\~a}o sobre fatores que exercem impacto sobre a produ{\c c}{\~a}o e reprodu{\c c}{\~a}o de desigualdades sociais e educacionais oscilou entre explica{\c c}{\~o}es otimistas sobre a expans{\~a}o dos sistemas escolares e outras que concedem peso {\`a} origem socioecon{\^o}mica dos alunos e/ou a fatores relacionados a processos escolares. O artigo discute a tradi{\c c}{\~a}o de estudos quantitativos que tiveram origem no Relat{\'o}rio Coleman (1966) e seus desdobramentos nos estudos de efeito escola e, posteriormente, de efeito vizinhan{\c c}a. Concentra sua an{\'a}lise nos limites e possibilidades trazidos pela literatura de efeito vizinhan{\c c}a e da geografia de oportunidades para a compreens{\~a}o de mecanismos mediadores entre a segrega{\c c}{\~a}o residencial e resultados escolares. Discute estudos que partiram desse arcabou{\c c}o te{\'o}rico e metodol{\'o}gico, focalizando o contexto brasileiro. Pontua os pr{\'o}ximos desafios para se avan{\c c}ar na compreens{\~a}o da organiza{\c c}{\~a}o social do territ{\'o}rio e as desigualdades educacionais nesse contexto.           ,              Depuis la p{\'e}riode d'apr{\`e}s-guerre, la discussion sur les facteurs qui exercent un impact sur la production et la reproduction des in{\'e}galit{\'e}s sociales et {\'e}ducatives ont oscill{\'e} entre des explications optimistes sur l'expansion des syst{\`e}mes scolaires et d'autres qui conc{\`e}dent un poids {\`a} l'origine socio-{\'e}conomique des {\'e}l{\`e}ves et/ou {\`a} des facteurs li{\'e}s {\`a} des processus scolaires. L'article discute la tradition des {\'e}tudes quantitatives qui s'origin{\`e}rent dans le Rapport Coleman (1966) et leurs pendants dans les {\'e}tudes de l'effet-{\'e}cole et, post{\'e}rieurement, de l'effet-voisinage. Il concentre son analyse sur les limites et les possibilit{\'e}s apport{\'e}es par la litt{\'e}rature de l'effet-voisinage et de la g{\'e}ographie des opportunit{\'e}s pour la compr{\'e}hension des m{\'e}canismes m{\'e}diateurs entre la s{\'e}gr{\'e}gation r{\'e}sidentielle et les r{\'e}sultats scolaires. Il discute des {\'e}tudes qui partirent de ce squelette th{\'e}orique et m{\'e}thodologique, en focalisant sur le contexte br{\'e}silien. Il souligne les prochains d{\'e}fis pour avancer dans la compr{\'e}hension de l'organisation sociale du territoire et les in{\'e}galit{\'e}s {\'e}ducatives dans ce contexte.           ,              Since the post war period, the discussion about the factor that impact the production and reproduction of social and educational inequalities has oscillated between optimistic views of school systems expansion and explanations that focus the effect of the student's socioeconomic background and/or the school processes. The article discusses a tradition of quantitative studies that began after the Coleman Report (1966) and their developments on the school-effect and the neighborhood-effect studies. It focuses on the limits and possibilities brought by neighborhood-effect and on the geography of opportunities literature in order to understand the mediation mechanisms between residential segregation and educational results. The article discusses researches that have used this theoretical and methodological framework considering the Brazilian context. It points to the next challenges to advance in the comprehension of the social organization of the territory and educational inequality in this context.},
  keywords = {/unread},
  annotation = {17 citations (Crossref) [2024-11-21]},
  file = {C:\Users\este.de.ahora\Documents\Biblioteca\Koslinski, Alves - 2012 - Novos olhares para as desigualdades de oportunidades educacionais.pdf}
